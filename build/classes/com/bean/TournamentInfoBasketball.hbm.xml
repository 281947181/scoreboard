<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.bean.TournamentInfoBasketball" table="tbl_tournament_info_basketball">
		<id name="vid">
			<generator class="native"></generator>
		</id>
		<property name="createTime"></property>
		<property name="setNum"></property>
		<property name="singleSetTime"></property>
		<property name="nameA"></property>
		<property name="nameB"></property>
		<property name="currentScoreA"></property>
		<property name="currentScoreB"></property>
		<property name="currentSetNum"></property>
		<property name="currentRestTimeMinute"></property>
		<property name="currentRestTimeSecond"></property>
		<property name="currentRestSideTime"></property>
	</class>
</hibernate-mapping>
	